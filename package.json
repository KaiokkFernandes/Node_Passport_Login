  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.7.7",
    "body-parse": "^0.1.0",
    "express": "^4.21.0",
    "module-alias": "^2.2.3",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
